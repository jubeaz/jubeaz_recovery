[Unit]
Description=start RedTeam linux_weaponize http server
After=network.target

[Service]
Type=simple
RemainAfterExit=yes
Restart=on-failure
ExecStart=python3 -m http.server 8883 --bind 127.0.0.1 --directory /opt/linux/linux_weaponize
WorkingDirectory=/opt/linux/linux_weaponize
User=http

[Install]
WantedBy=multi-user.target

